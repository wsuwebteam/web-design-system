{% macro headerUtilityBar(  classes='', link='#', title='Give' ) %}
<div class="wsu-header-utility-bar {{ classes }}" >
    <button class="wsu-header-utility-bar__quicklinks wsu-slide-in-panel--open" data-panel="wsu-quicklinks-panel" id="open-modal">
        Quicklinks / Search
        <i class="wsu-header-utility-bar__icon"></i>
    </button>
    <a href="{{link}}" class="wsu-header-utility-bar__cta">{{title}}</a>

</div>
{% endmacro %}