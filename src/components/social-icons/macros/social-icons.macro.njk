{% macro socialIcons( modifierClass='', twitter=true, faceblook=true, linkedin=true, pintrest=true, email=true, youtube=true, instagram=true ) %}
<ul class="wsu-social-icons{{ modifierClass }}">
    {% if twitter %}
    <li class="wsu-social-icons__twitter">
        <a href="#"><span class="screen-reader-only">Share on Twitter</span></a>
    </li>
    {% endif %}
    {% if faceblook %}
    <li class="wsu-social-icons__faceblook">
        <a href="#"><span class="screen-reader-only">Share on FaceBook</span></a>
    </li>
    {% endif %}
    {% if linkedin %}
    <li class="wsu-social-icons__linkedin">
        <a href="#"><span class="screen-reader-only">Share on Linkedin</span></a>
    </li>
    {% endif %}
    {% if pintrest %}
    <li class="wsu-social-icons__pintrest">
        <a href="#"><span class="screen-reader-only">Share on pinterest</span></a>
    </li>
    {% endif %}
    {% if youtube %}
    <li class="wsu-social-icons__youtube">
        <a href="#"><span class="screen-reader-only">Share on YouTube</span></a> 
    </li>
    {% endif %}
    {% if instagram %}
    <li class="wsu-social-icons__instagram">
        <a href="#"><span class="screen-reader-only">Share on Instagram</span></a>
    </li>
    {% endif %}
    {% if email %}
    <li class="wsu-social-icons__email">
        <a href="mailto:?subject=[title]&body=Check out this: [url]"><span class="screen-reader-only">Share on Email</span></a>
    </li>
    {% endif %}
</ul>
{% endmacro %} 
